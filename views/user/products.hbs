<section>
  <div class="featured mt-4">
    <h2>Recommemded <b>Products</b></h2>
  </div>
  <div class="container d-flex justify-content-center mt-50 mb-50">

    <div class="row">
      {{#each products}}
        <div class="col-md-4 mt-2">

        <div class="card">
          <div class="imgcard-body">
            <div class="card-img-actions">

              <img
                src="/product-images/{{this._id}}.jpg"
                class="card-img img-fluid"
                alt=""
              />

            </div>
          </div>

          <div class="card-body bg-light text-center">
            <div class="mb-2">
              <h6 class="font-weight-semibold mb-2">
                <a href="#" class="text-default mb-2" data-abc="true">{{this.Name}}</a>
              </h6>

              <a href="#" class="text-muted" data-abc="true">{{this.Description}}</a>
            </div>

            <h3 class="mb-0 font-weight-semibold">{{this.Price}}</h3>

            <div>
              <i class="fa fa-star star"></i>
              <i class="fa fa-star star"></i>
              <i class="fa fa-star star"></i>
              <i class="fa fa-star star"></i>
            </div>

            <div class="text-muted mb-3">34 reviews</div>

            <button onclick="addToCart('{{this._id}}')" type="button" class="bg-cart"><i
                class="fa fa-cart-plus mr-2"
              ></i>
              Add to cart</button>

          </div>
        </div>

      </div>
      {{/each}}

    </div>
  </div>
</section>
